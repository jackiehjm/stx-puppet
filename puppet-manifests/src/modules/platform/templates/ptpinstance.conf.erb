[global]
##
## Default Data Set
##
<%- if @global_parameters -%>
<%- @global_parameters.each do |key,value| -%>
<%= key %> <%= value %>
<%- end -%>
<%- end -%>

<%- if @interfaces -%>
<%- @interfaces.each do |interface| %>
<%- interface['port_names'].each do |port| -%>
[<%= port %>]
##
## Associated interface: <%= interface['_ifname'] %>
##
<%- interface['parameters'].each do |key,value| -%>
<%= key %> <%= value %>



<%- end -%>
<%- end -%>
<%- end -%>
<%- end -%>
